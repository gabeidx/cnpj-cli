{
  "name": "cnpj-cli",
  "version": "2.1.0",
  "description": "A command line interface for generating, validating and formatting CNPJs",
  "type": "module",
  "main": "dist/index.js",
  "files": [
    "dist/index.js"
  ],
  "bin": {
    "cnpj": "dist/index.js"
  },
  "repository": "gabrielizaias/cnpj-cli",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "prebuild": "del ./dist",
    "build": "tsc",
    "postbuild": "chmod +x dist/index.js",
    "lint": "prettier --write '**/*.ts'",
    "prepublishOnly": "npm run build",
    "pretest": "npm run build",
    "test": "ava"
  },
  "dependencies": {
    "cnpj": "^4.0.1",
    "meow": "^10.1.1",
    "update-notifier": "^5.1.0"
  },
  "devDependencies": {
    "@types/node": "^16.10.1",
    "@types/update-notifier": "^5.1.0",
    "ava": "^3.15.0",
    "del-cli": "^4.0.1",
    "execa": "^5.1.1",
    "prettier": "^2.4.1",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  },
  "keywords": [
    "cli-app",
    "cli",
    "cnpj"
  ],
  "license": "MIT"
}
