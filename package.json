{
  "name": "cnpj-cli",
  "version": "3.0.0",
  "description": "A command line interface for generating, validating and formatting CNPJs",
  "type": "module",
  "main": "dist/index.js",
  "files": [
    "dist/index.js"
  ],
  "bin": {
    "cnpj": "dist/index.js"
  },
  "repository": "gabrielizaias/cnpj-cli",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "prebuild": "del ./dist",
    "build": "tsc",
    "postbuild": "chmod +x dist/index.js",
    "lint": "prettier --write '**/*.ts'",
    "prepublishOnly": "npm run build",
    "pretest": "npm run build",
    "test": "ava"
  },
  "dependencies": {
    "cnpj": "4.0.3",
    "meow": "11.0.0",
    "update-notifier": "6.0.2"
  },
  "devDependencies": {
    "@types/node": "18.15.11",
    "@types/update-notifier": "6.0.2",
    "ava": "5.2.0",
    "del-cli": "5.0.0",
    "execa": "7.1.1",
    "prettier": "2.8.7",
    "ts-node": "10.9.1",
    "typescript": "5.0.4"
  },
  "keywords": [
    "cli-app",
    "cli",
    "cnpj"
  ],
  "license": "MIT"
}
